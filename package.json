{
  "name": "yesoperator-website",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf public && rimraf ./themes/yesoperator/assets/css/index/css",
    "start": "TAILWIND_MODE=watch NODE_ENV=development npm-run-all --print-label clean prelim:twcss --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all --print-label clean prelim:twcss prod:twcss prod:hugo",
    "prelim:twcss": "./node_modules/tailwindcss/lib/cli.js -i ./themes/yesoperator/assets/css/tw-input.css -o ./themes/yesoperator/assets/css/tw-output.css --jit",
    "dev:twcsswatch": "./node_modules/tailwindcss/lib/cli.js -i ./themes/yesoperator/assets/css/tw-input.css -o ./themes/yesoperator/assets/css/tw-output.css --jit -w",
    "dev:hugo": "hugo server -w",
    "prod:twcss": "./node_modules/tailwindcss/lib/cli.js -i ./themes/yesoperator/assets/css/tw-input.css -o ./themes/yesoperator/assets/css/tw-output.css --jit --minify",
    "prod:hugo": "hugo --gc --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.1"
  }
}
